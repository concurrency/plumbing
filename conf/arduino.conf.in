# Default Arduino configuration: ATmega328 running a 5V
TVM_MCU=m328p
TVM_BYTECODE_ADDR=0x5000
TVM_F_CPU=16000000
TVM_UPLOAD_RATE=57600
PLATFORM="@PLATFORM@"

# TVM_INST_ROOT=@FINAL@
     
# avrdude
TVM_AVRDUDE_FIRMWARE_FLAGS="-V -F -p $TVM_MCU"
TVM_AVRDUDE_CODE_FLAGS="-V -F -p $TVM_MCU -b $TVM_UPLOAD_RATE -c stk500v1"
TVM_AVRDUDE_CONF="@FINAL_DEST_CONF@/avrdude.conf"
TVM_ARDUINO_FIRMWARE=tvm-avr-atmega328p-16000000.hex

