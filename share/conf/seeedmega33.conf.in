# The Seeed Studio Mega running a 3.3V
TVM_MCU=m1280
TVM_BYTECODE_ADDR=0x5000
TVM_F_CPU=8000000
TVM_UPLOAD_RATE=57600
PLATFORM="@PLATFORM@"

TVM_INST_ROOT=@FINAL@
     
# avrdude
TVM_AVRDUDE_FIRMWARE_FLAGS="-V -F -p $TVM_MCU"
TVM_AVRDUDE_CODE_FLAGS="-V -F -p $TVM_MCU -b $TVM_UPLOAD_RATE -c stk500v1"
TVM_AVRDUDE_CONF="$TVM_INST_ROOT/share/conf/avrdude.conf"
TVM_ARDUINO_FIRMWARE=tvm-avr-atmega1280-8000000.hex

